      <tr><td><b>brili</b>, <i>to shine</i></td><td><b>brilo</b>, <i>shine, brilliance</i></td><td><b>brila</b>, <i>shining, brilliant</i></td><td><b>brile</b>, <i>brilliantly</i></td></tr>
      <tr><td><b>flori</b>, <i>to bloom</i></td><td><b>floro</b>, <i>flower, blossom</i></td><td><b>flora</b>, <i>floral</i></td><td><b>flore</b>, <i>florally</i></td></tr>
